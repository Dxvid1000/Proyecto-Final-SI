    <div class="card" style="width:400px">
      <h4 class="card-title">Marca: {{datos.marca}}</h4>
      <img class="card-img-top" src="{{datos.imagen}}" alt="Card image" style="width:100%">
      <div class="card-body">
      <p class="card-text"> Submarca: {{datos.submarca}}</p>
      </div>
    </div>
  </div>
</div>
</body>
</html>